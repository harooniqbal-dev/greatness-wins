{% assign min = 1 %}
{% assign max = 1000 %}
{% assign diff = max | minus: min %}
{% assign randomNumber = "now" | date: "%N" | modulo: diff | plus: min %}

<svg id="icon__plus-{{ randomNumber }}" width="9" height="9" viewBox="0 0 9 9" fill="none" xmlns="http://www.w3.org/2000/svg"><path stroke="#000" d="M0 4.5h9M4.5 0v9"/></svg>
