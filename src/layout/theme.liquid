    </style>
    <link
      rel="preload"
      href="{{ 'custom_styles.css' | asset_url }}"
      as="style"
      onload="this.onload=null;this.rel='stylesheet'">
    <noscript><link rel="stylesheet" href="{{ 'custom_styles.css' | asset_url }}"></noscript>
    {{ content_for_header }}

    {%- if request.page_type == 'collection' or request.page_type == 'search' -%}
      {%- include 'boost-pfs-style' -%}
    {%- endif -%}

    <script type="application/ld+json">
      {
      "@context": "https://schema.org",
                                        "@type": "WebSite",
                                        "@id": "{{ shop.secure_url }}/#website",
      "url": "{{ shop.secure_url }}",
      "potentialAction": {
                                            "@type": "SearchAction",
                                            "target": "{{ shop.secure_url }}/search?q={query}",
      "query-input": "required name=query"
                                        }
                                    }
    </script>
    <script type="application/ld+json">
      {
      "@context": "https://schema.org",
                                        "@type": "Organization",
                                        "@id": "{{ shop.secure_url }}/#org",
      "name": "{{ shop.name }}",
      "url": "{{ shop.secure_url }}",
      "description": "{{ shop.description }}",
      "telephone": "{{ shop.phone }}",
      "logo": "{{ settings.logo_dark | img_url }}",
      "image": "{{ settings.logo_dark | img_url }}",
      "address": {
                                            "@type": "PostalAddress",
                                            "streetAddress": "{{ shop.address.street }}",
      "addressLocality": "{{ shop.address.city }}",
      "addressRegion": "{{ shop.address.province }}",
      "postalCode": "{{ shop.address.zip }}",
      "addressCountry": "{{ shop.address.country }}"
      }
                                    }
    </script>

    <!-- YOTPO JAVASCRIPT | location: layout > theme.liquid  -->
    <script type="text/partytown">
      (function e(){
      var e=document.createElement("script");
                                            e.type="text/javascript",e.async=true,e.src="//staticw2.yotpo.com/UkCHsjiu6UpW1ZMqTBR8woLNu0isrYeXSs5lbFfO/widget.js";
                                            var t=document.getElementsByTagName("script")[0];
                                            t.parentNode.insertBefore(e,t)
                                        })();
    </script>
    <!-- END | YOTPO JAVASCRIPT -->
  </head>

  <body class="overflow-x-hidden">
    <img
      width="99999"
      height="99999"
      style="pointer-events: none; position: absolute; top: 0; left: 0; width: 99vw; height: 99vh; max-width: 99vw; max-height: 99vh;"
      src="data:image/svg+xml;base64,">

    <ul class="skip-links absolute top-2 left-2">
      <li>
        <a class="skip-links__item js-skip-link" href="#main-navbar">{{ 'navigation.skip_links.nav' | t }}</a>
      </li>
      <li class="hidden lg:block">
        <a class="skip-links__item js-skip-link" href="#search">{{ 'navigation.skip_links.search' | t }}</a>
      </li>
      <li>
        <a class="skip-links__item js-skip-link" href="#main-content">{{ 'navigation.skip_links.main' | t }}</a>
      </li>
      <li>
        <a class="skip-links__item js-skip-link" href="#main-footer">{{ 'navigation.skip_links.footer' | t }}</a>
      </li>
    </ul>

    <header class="main-header w-full fixed top-0 left-0 right-0 transition-transform z-20 js-main-header" id="header">
      <div class="flex justify-center bg-[#C4C4C4] text-black w-full text-center transition-[max-height] h-11 js-top-banner">
        <div class="container relative xl:max-w-[83.25rem] flex justify-center items-center py-2">
          <p class="text-sm sm:text-base lg:text-lg font-semibold">{{ 'navigation.top_banner_text' | t }}</p>
          <button aria-label="{{ 'navigation.top_banner_close_button_label' | t }}" class="top-banner__button absolute right-2 lg:right-0 top-1/2 -translate-y-1/2 focus-visible:outline-2 outline-offset-2 outline-orange js-top-banner-button">
            <span aria-hidden="true">{%- render 'i-close' -%}</span>
          </button>
        </div>
      </div>
      {%- section 'navbar' -%}
    </header>

    <main
      class="main-content main-content--additional-margin js-main-content"
      style="margin-top: -0px;"
      id="main-content">
      {%- if template.name == "index" -%}
        <h1 class="sr-only">{{ page_title }}</h1>
      {%- endif -%}
      {{ content_for_layout }}
    </main>

    <footer class="js-main-footer" id="main-footer">
      {%- section 'footer' -%}
    </footer>

    {%- render 'variables-js' -%}

    <script src="{{ 'default_page.js' | asset_url }}" defer></script>

    {%- if request.page_type == 'collection' or request.page_type == 'search' -%}
      {%- include 'boost-pfs' -%}
      <script src="{{ 'custom_quick_buy_scripts.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- if request.page_type == 'product' -%}
      <script src="{{ 'product_page.js' | asset_url }}" defer></script>
    {%- endif -%}


    {%- if request.page_type == 'collection' -%}
      {% if collection.metafields.my_fields.show_promo_banner %}

        <script
          crossorigin="anonymous"
          integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
          src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
        {% if collection.handle == "mens-compression-bottoms" or collection.handle == "mens-compression-tops" %}
          <script src="{{ 'custom_scripts.js' | asset_url }}" defer></script>
        {% else %}
          <script src="{{ 'custom_script_additional.js' | asset_url }}" defer></script>
        {% endif %}

      {% endif %}
    {%- endif -%}

    {%- if request.page_type == 'blog' or request.page_type == 'article' -%}
      <script src="{{ 'blog_page.js' | asset_url }}" defer></script>
    {%- endif -%}
  </body>
</html>