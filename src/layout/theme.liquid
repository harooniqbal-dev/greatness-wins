    <style>
      .intro-gw-sec p{font-weight:normal;}
.intro-gw-sec { padding:50px 0; }
.collection-header > div h2 {
    max-width: 350px;
    font-size: 36px;
}

.collection-item-wrapper {
    margin-bottom: 30px;
}
.cust-em-logo {
    text-align: center;
    margin-bottom: 50px;
}
.cust-em-logo img {
    display: inline-block;
    max-width: 200px;
}
.product-type-promo {
    background: #f6f6f6;
    text-align: center;
    color: #000;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    text-transform: uppercase;
    padding: 0 15px;
}

.product-type-promo h2 {
    font-size: 16px;
    line-height: 18px;
    font-weight: bold;
    max-width: 280px;
}

.product-type-promo p {
    font-weight: bold;
    font-size: 12px;
    line-height: 18px;
    text-transform: none;
    margin-top: 16px;
    max-width: 90%;
}

.collection-header > div {
    position: absolute;
    top: 30%;
    left: 0;
    right: 0;
}

.product-type-promo[data-product-title="Core Tech Quarter Zip"] h2,
.product-type-promo[data-product-title="Performance Training Quarter Zip"] h2 {
    max-width: 250px;
}

.cart-items-count {
    background-color: #2f2f2f;
    color: #fff;
    position: absolute;
    width: 20px;
    height: 20px;
    border-radius: 20px;
    font-size: 12px;
    top: -10px;
    text-align: center;
}
img[alt="Chris signature"] {
    margin-left: 0;
    max-width: 280px;
}
.smartwishlist {
    font-size: 12px !important;
    position: absolute;
    display: none !important;
}
/* chat icon hide */

iframe#dummy-chat-button-iframe {
    display: none !important;
}
/* chat icon hide */
@media only screen and (min-width: 768px) {
    .product-type-promo h2 {
        font-size: 24px;
        line-height: 30px;
    }

    .product-type-promo p {
        margin-top: 32px;
    }
}
div#smartwishlist {
    display: none !important;
}
.smartwishlist.pro-thumb-swlist{
    position: absolute;
    z-index: 1 !important;
    left: 10px;
    top: 5px;
    margin: 0;
    font-size: 20px !important;
    display: none !important;
}
