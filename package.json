{
    "name": "crunch-shopify",
    "version": "0.0.1",
    "description": "Crunch Shopify starter theme based on TailwindCSS",
    "main": "src/scripts/theme.js",
    "scripts": {
        "start": "npm run watch",
        "deploy": "rm -rf dist && webpack --mode production && node themekit.deploy.js",
